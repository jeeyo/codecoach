     - yarn lint || true
+  artifacts:
+    when: on_success
+    paths:
+      - ./eslint-report.json
+    reports:
+      dotenv: env.txt
+
+codecoach-on-mr:
+  extends: [.codecoach]
+  stage: lint
+  needs: [eslint-on-mr]
+  rules:
+    - if: $CI_PIPELINE_SOURCE == "push" && $CI_COMMIT_BRANCH == $CI_DEFAULT_BRANCH
+      when: never
+    - if: $CI_PIPELINE_SOURCE == "merge_request_event"
+  variables:
+    LINT_TYPE: eslint
+    LINT_OUTPUT: eslint-report.json
+    LINT_CONTEXT: ${ESLINT_CONTEXT}
+
+codecoach-game:
+  stage: lint
+  rules:
+    - if: $CI_PIPELINE_SOURCE == "push" && $CI_COMMIT_BRANCH == $CI_DEFAULT_BRANCH
+      when: never
+    - if: $CI_PIPELINE_SOURCE == "merge_request_event"
+  needs: [codecoach-on-mr]
+  image: reg-hk.agodadev.io/dockerhub/library/node:18-alpine
+  script:
+    - apk add curl --no-cache
+
+    # default branch codecoach result
+    - |
+      curl \
+        --header "PRIVATE-TOKEN: ${CODECOACH_TOKEN}" \
+        "https://gitlab.agodadev.io/api/v4/projects/${CI_PROJECT_ID}/jobs/artifacts/${CI_DEFAULT_BRANCH}/raw/output.json?job=codecoach" \
+        -o default_branch.json
+    - cat default_branch.json
+
+    # mr codecoach result
+    - cat output.json
